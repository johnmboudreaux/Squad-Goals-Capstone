{
  "name": "squad-goals",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.1.2"
  },
  "proxy": "http://localhost:4000",
  "main": "index.js",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "server": "nodemon server.js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "npm-run-all testserver testreactci",
    "testreactci": "CI=true react-scripts test --env=jsdom",
    "testreact": "react-scripts test --env=jsdom",
    "testserver": "NODE_ENV=test mocha --exit",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "firebase": "^4.8.1",
    "jest-cli": "^22.0.3",
    "knex": "^0.14.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "git+https://github.com/chaijs/chai-http.git#3ea4524",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.3",
    "mocha": "^4.0.1",
    "npm-watch": "^0.3.0"
  }
}
